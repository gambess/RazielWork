<?php

namespace Pi2\Fractalia\SmsBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Smsresumen
 *
 * @ORM\Table(name="SmsResumen", indexes={@ORM\Index(name="fk_SmsResumen_MensajeResumen1", columns={"resumen_id"})})
 * @ORM\Entity
 */
class Smsresumen
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $id;

    /**
     * @var \Pi2\Fractalia\SmsBundle\Entity\Resumen
     *
     * @ORM\ManyToOne(targetEntity="Pi2\Fractalia\SmsBundle\Entity\Resumen")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="resumen_id", referencedColumnName="id")
     * })
     */
    private $resumen;


}
