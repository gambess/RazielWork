<?php

namespace Pi2\Fractalia\SmsBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Columnaevento
 *
 * @ORM\Table(name="ColumnaEvento", indexes={@ORM\Index(name="fk_ColumnaEvento_Mensaje1", columns={"mensaje_id"})})
 * @ORM\Entity
 */
class Columnaevento
{
    /**
     * @var string
     *
     * @ORM\Column(name="numero_caso", type="string", length=20, nullable=false)
     */
    private $numeroCaso;

    /**
     * @var string
     *
     * @ORM\Column(name="cliente", type="string", length=50, nullable=false)
     */
    private $cliente;

    /**
     * @var string
     *
     * @ORM\Column(name="tipo", type="string", length=50, nullable=false)
     */
    private $tipo;

    /**
     * @var string
     *
     * @ORM\Column(name="tecnico", type="string", length=100, nullable=false)
     */
    private $tecnico;

    /**
     * @var string
     *
     * @ORM\Column(name="tsol", type="string", length=10, nullable=false)
     */
    private $tsol;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="fecha", type="datetime", nullable=false)
     */
    private $fecha;

    /**
     * @var string
     *
     * @ORM\Column(name="modo", type="string", length=10, nullable=false)
     */
    private $modo;

    /**
     * @var string
     *
     * @ORM\Column(name="detalle", type="text", nullable=false)
     */
    private $detalle;

    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $id;

    /**
     * @var \Pi2\Fractalia\SmsBundle\Entity\Mensaje
     *
     * @ORM\ManyToOne(targetEntity="Pi2\Fractalia\SmsBundle\Entity\Mensaje")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="mensaje_id", referencedColumnName="id")
     * })
     */
    private $mensaje;


}
