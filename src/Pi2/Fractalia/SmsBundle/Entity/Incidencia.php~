<?php

namespace Pi2\Fractalia\SmsBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Incidencia
 *
 * @ORM\Table(name="Incidencia")
 * @ORM\Entity
 */
class Incidencia
{
    /**
     * @var string
     *
     * @ORM\Column(name="numero_caso", type="string", length=20, nullable=false)
     */
    private $numeroCaso;

    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $id;


}
