<?php

namespace Pi2\Fractalia\SmsBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Columnaresumen
 *
 * @ORM\Table(name="ColumnaResumen", indexes={@ORM\Index(name="fk_ColumnaResumen_MensajeResumen1", columns={"resumen_id"})})
 * @ORM\Entity
 */
class Columnaresumen
{
    /**
     * @var string
     *
     * @ORM\Column(name="numero_caso", type="string", length=45, nullable=false)
     */
    private $numeroCaso;

    /**
     * @var string
     *
     * @ORM\Column(name="estado", type="string", length=45, nullable=false)
     */
    private $estado;

    /**
     * @var string
     *
     * @ORM\Column(name="servicio", type="string", length=45, nullable=false)
     */
    private $servicio;

    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $id;

    /**
     * @var \Pi2\Fractalia\SmsBundle\Entity\Resumen
     *
     * @ORM\ManyToOne(targetEntity="Pi2\Fractalia\SmsBundle\Entity\Resumen")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="resumen_id", referencedColumnName="id")
     * })
     */
    private $resumen;


}
