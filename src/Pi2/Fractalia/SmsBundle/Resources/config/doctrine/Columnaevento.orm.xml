<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Pi2\Fractalia\SmsBundle\Entity\Columnaevento" table="ColumnaEvento">
    <indexes>
      <index name="fk_ColumnaEvento_Mensaje1" columns="mensaje_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="numeroCaso" type="string" column="numero_caso" length="20" nullable="false"/>
    <field name="cliente" type="string" column="cliente" length="50" nullable="false"/>
    <field name="tipo" type="string" column="tipo" length="50" nullable="false"/>
    <field name="tecnico" type="string" column="tecnico" length="100" nullable="false"/>
    <field name="tsol" type="string" column="tsol" length="10" nullable="false"/>
    <field name="fecha" type="datetime" column="fecha" nullable="false"/>
    <field name="modo" type="string" column="modo" length="10" nullable="false"/>
    <field name="detalle" type="text" column="detalle" nullable="false"/>
    <many-to-one field="mensaje" target-entity="Mensaje">
      <join-columns>
        <join-column name="mensaje_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
