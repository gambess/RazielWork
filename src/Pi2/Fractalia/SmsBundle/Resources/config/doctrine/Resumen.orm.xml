<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Pi2\Fractalia\SmsBundle\Entity\Resumen" table="Resumen">
    <indexes>
      <index name="fk_MensajeResumen_Incidencia1" columns="incidencia_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="texto" type="text" column="texto" nullable="false"/>
    <many-to-one field="incidencia" target-entity="Pi2\Fractalia\Entity\SGSD\Incidencia">
      <join-columns>
        <join-column name="incidencia_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
