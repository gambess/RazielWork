<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="wsdl" pattern="/wsdl">
        <default key="_controller">Pi2FracSGSDSoapServerBundle:Default:wsdl</default>
    </route>
     <route id="xsd" pattern="/xsd">
        <default key="_controller">Pi2FracSGSDSoapServerBundle:Default:xsd</default>
    </route>
    
    <route id="soap_service" pattern="/GINotificarEvento">
        <default key="_controller">Pi2FracSGSDSoapServerBundle:Default:soapService</default>
    </route>
</routes>
